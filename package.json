{
  "name": "safelink",
  "version": "0.5.11",
  "private": false,
  "author": "Joel Grenon <jgrenon@vibes.io>",
  "description": "SafeLink is an open-source NodeJS library created to maintain long-term communication between distant remote sites with varying network quality",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgrenon/safelink.git"
  },
    "publishConfig": {
        "registry": "http://npm.vibes.io/"
    },

  "keywords": [
    "messaging",
    "remoting",
    "command",
    "response",
    "events",
    "heartbeat",
    "long-polling"
  ],
  "dependencies": {
    "async": "~0.2.9",
    "q": "~0.9.6",
    "moment": "~2.1.0",
    "bunyan": "~0.21.4",
    "redis": "~0.8.4",
    "hiredis": "~0.1.15",
    "request": "~2.27.0",
    "shortid": "~2.0.0",
      "lodash": "~2.2.1",
    "underscore-query": "http://npm.vibes.io/package/underscore-query/latest"
  },
  "license": "Apache V2.0",
  "engines": {
    "node": ">=0.10.x"
  }
